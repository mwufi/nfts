{{#each folders ~}}
    import {{this}} from './{{this}}'
{{/each}}

{{#each files ~}}
    import { {{this}}Template, {{this}} } from './{{this}}'
{{/each}}

export default {
    {{#each folders}}
        {{ this }},
    {{/each}}
    {{#each files}}
        {{ this }}, {{ this }}Template,
    {{/each}}
}